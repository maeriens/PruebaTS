{
  "compilerOptions": {
    "jsx": "react",
    "target": "es5", 
    "module": "commonjs", 
    "allowJs": true, 
    "sourceMap": true,
    "removeComments": true,
    "noEmit": true, 
    "allowSyntheticDefaultImports": true, 
    "esModuleInterop": true, 
     "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "build"
  },
  "include": [ "src" ]
}